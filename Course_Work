    "year_cons_standings = f1.constructor_standings(season=2013).json['MRData']['StandingsTable']['StandingsLists'][0]['ConstructorStandings']\n",
    "year_cons_standings_stripped = []\n",
    "for constructor in year_cons_standings:\n",
    "    constructor_stripped = {}\n",
    "    for key, values in constructor.items():\n",
    "        if key != 'Constructor':\n",
    "            constructor_stripped[key] = values\n",
    "        else:\n",
    "            constructor_stripped['name'] = values['name']\n",
    "            constructor_stripped['nationality'] = values['nationality']\n",
    "    year_cons_standings_stripped.append(constructor_stripped)\n",
    "    \n",
    "year_cons_standings_stripped"
